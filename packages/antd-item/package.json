{
  "name": "@event-chat/antd-item",
  "version": "0.3.2",
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rslib build",
    "build:storybook": "storybook build",
    "dev": "rslib build --watch",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx}\"",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx,js,jsx}\" --fix",
    "storybook": "storybook dev",
    "test": "rstest",
    "test:all": "tsd && rstest --coverage",
    "test:coverage": "rstest --coverage",
    "test:type": "tsd"
  },
  "devDependencies": {
    "@event-chat/core": "workspace:*",
    "@rslib/core": "^0.18.4",
    "@rstest/core": "^0.7.1",
    "@rstest/coverage-istanbul": "^0.1.3",
    "@storybook/addon-docs": "^10.1.4",
    "@storybook/addon-onboarding": "^10.1.4",
    "@storybook/react": "^10.1.4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "antd": "^6.1.3",
    "jsdom": "^26.1.0",
    "storybook": "^10.1.4",
    "storybook-addon-rslib": "^3.1.0",
    "storybook-react-rsbuild": "^3.1.0",
    "tsd": "^0.33.0",
    "typescript-eslint": "^8.48.0"
  },
  "peerDependencies": {
    "@event-chat/core": "*",
    "antd": "^4.0.0 || ^5.0.0 || ^6.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "zod": "^3.0.0 || ^4.0.0"
  }
}
