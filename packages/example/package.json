{
  "name": "@event-chat/example",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm run lint && rsbuild build",
    "dev": "rsbuild dev --open",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx}\"",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx,js,jsx}\" --fix",
    "preview": "rsbuild preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@event-chat/antd-item": "workspace:*",
    "@event-chat/core": "workspace:*",
    "@fortawesome/free-regular-svg-icons": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/react-fontawesome": "^3.1.1",
    "@types/lodash": "^4.17.21",
    "antd": "5.14.0",
    "lodash": "^4.17.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@ant-design/cssinjs": "^2.0.2",
    "@chromatic-com/storybook": "^4.1.3",
    "@storybook/addon-a11y": "^10.1.10",
    "@storybook/addon-docs": "^10.1.8",
    "@storybook/addon-onboarding": "^10.1.8",
    "@storybook/addon-themes": "^10.1.10",
    "@storybook/react": "^10.1.8",
    "@tailwindcss/postcss": "^4.1.18",
    "storybook": "^10.1.8",
    "storybook-react-rsbuild": "^3.1.0",
    "tailwindcss": "^4.1.18"
  }
}
